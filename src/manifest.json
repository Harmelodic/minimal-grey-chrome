{
    "manifest_version": 2,
    "name": "Minimal Grey",
    "version": "1.0.1",
    "description": "A minimalist, grey Chrome theme",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "theme": {
        "colors": {
            "frame": [78,78,78],
            "toolbar": [235,235,235],
            "tab_text": [48,48,48],
            "tab_background_text": [48,48,48],
            "ntp_background": [255,255,255],
            "button_background": [163,163,163,1]
        },
        "properties": {
            "ntp_background_alignment": "bottom",
            "ntp_background_repeat": "no-repeat"
        },
        "tints": {
            "buttons": [0,0,0.2]
        }
    }
}